<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.VBox?>

<SplitPane id="root" fx:id="splitPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="net.core.coremusic.SettingsController">
   <items>
      <ScrollPane fitToHeight="true" fitToWidth="true">
         <content>
            <VBox>
               <children>
                  <ToggleButton fx:id="themesBtn" mnemonicParsing="false" styleClass="item" text="Themes" />
                  <ToggleButton fx:id="musicDirBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" styleClass="item" text="Music Directory" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </items>
</SplitPane>
